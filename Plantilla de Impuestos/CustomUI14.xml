<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon.CargarCinta">
	<ribbon startFromScratch="false">
		<tabs>
			<tab id="Tab1" label="Registro">
				
				<group id="Group1" label="Resumen">
					<button id="Button1" label="Principal" image="Tienda_32"  size="large"  getEnabled="Ribbon.DesactivarBoton1" onAction="Boton1" />		
				</group>
							
				<group id="Group2" label="Datos">			
					<button id="Button2" label="Factura" image="Comi1"  size="large"  getEnabled="Ribbon.DesactivarBoton2" onAction="Boton2" />					
					<button id="Button3" label="Nota" image="file_32" size="large"   getEnabled="Ribbon.DesactivarBoton3" onAction="Boton3" />	
					<button id="Button4" label="Cierre" image="excel1" size="large"   getEnabled="Ribbon.DesactivarBoton4" onAction="Boton4" />											
				</group>


				<group id="Group3" label="Guardar">
					<button id="Button5" label="Guardar" image="Save_32"  size="large"  getEnabled="Ribbon.DesactivarBoton5" onAction="Boton5" />		
				</group>
			
			</tab>
		</tabs>
	</ribbon>
</customUI>
